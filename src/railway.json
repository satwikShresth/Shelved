{
  "$schema": "https://railway.com/railway.schema.json",
  "build": {
    "builder": "DOCKERFILE",
    "dockerfilePath": "src/dockerfile"
  },
  "deploy": {
    "runtime": "V2",
    "numReplicas": 1,
    "limitOverride": {
      "containers": {
        "cpu": 3,
        "memoryBytes": 3000000000
      }
    },
    "sleepApplication": true,
    "multiRegionConfig": {
      "us-east4-eqdc4a": {
        "numReplicas": 1
      }
    },
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10
  }
}
